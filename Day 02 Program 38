PATTERN-04
#include <stdio.h>

int main() {
    int n, count = 1;
    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {        
        for (int j = 1; j <= n; j++) {     
            if (i == 1 || i == n || j == 1 || j == n) {
                printf("%-5d", count);      
            } else {
                printf("     ");           
            }
            count++;
        }
        printf("\n");
    }

    return 0;
}

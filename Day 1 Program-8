Math Functions
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main()
    {
    float a;
    int b,c,d;
    scanf("%f", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    scanf("%d", &d);
    printf("%d",(int)floor(a));
    printf("%d",(int)ceil(a));
    printf("%d",(int)sqrt(b));
    prinft("%d",(int)pow(c,d));
    return 0;
    }


    
    
    

